{
  "name": "datashare-plugin-tour",
  "description": "A Datashare plugin to build a tour guide",
  "version": "0.0.0",
  "main": "dist/index.umd.min.js",
  "style": "dist/index.css",
  "dependencies": {
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.2"
  },
  "devDependencies": {
    "@vue/cli": "^4.2.3",
    "@vue/cli-service-global": "^4.2.3",
    "geckodriver": "^1.19.1",
    "nightwatch": "^1.3.4"
  },
  "private": false,
  "datashare": {},
  "scripts": {
    "test:e2e": "nightwatch",
    "test:pack": "mkdir -p ./tmp && cat \"$(npm pack)\" | tar zxvf - -C ./tmp",
    "test:datashare": "datashare --tcpListenPort 8008 --pluginsDir=$(pwd)/tmp",
    "test:clean": "rm *.tgz || /bin/true",
    "test": "yarn test:e2e",
    "pretest": "yarn build && yarn test:pack",
    "posttest": "yarn test:clean",
    "build": "npx vue build --target lib --name index",
    "prepublishOnly": "yarn build"
  },
  "author": "annelhote",
  "license": "MIT",
  "vue": {
    "filenameHashing": false
  },
  "files": [
    "dist/{css,js}/*.{css,js,map}"
  ]
}
